CREATE TABLE Users (
    UserID INTEGER PRIMARY KEY,
    Name TEXT,
    Password TEXT,
    ConfirmPassword TEXT,
    Phone TEXT,
    Email TEXT
);

CREATE TABLE Habits (
    HabitID INTEGER PRIMARY KEY,
    UserID INTEGER,
    Habit TEXT,
    Date TEXT,
    FOREIGN KEY(UserID) REFERENCES Users(UserID)
);

